      <li Class="PatchOperationAdd" MayRequire="VanillaExpanded.VanillaTraitsExpanded">
        <xpath>Defs/TraitDef[defName="VTE_AbsentMinded" or defName="VTE_Submissive"]/modExtensions/li[@Class="PawnsChooseResearch.ResearchCategory"]</xpath>
        <value>
          <progressTech>1</progressTech>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="VanillaExpanded.VanillaTraitsExpanded">
        <xpath>Defs/TraitDef[defName="VTE_Eccentric" or defName="VTE_Perfectionist" or defName="VTE_Prodigy"]/modExtensions/li[@Class="PawnsChooseResearch.ResearchCategory"]</xpath>
        <value>
          <progressTech>-1</progressTech>
        </value>
      </li>
      <!-- Tech Level -->
      <li Class="PatchOperationAdd" MayRequire="VanillaExpanded.VanillaTraitsExpanded">
        <xpath>Defs/TraitDef[defName="VTE_Eccentric" or defName="VTE_Academian"]/modExtensions/li[@Class="PawnsChooseResearch.ResearchCategory"]</xpath>
