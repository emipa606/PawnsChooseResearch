<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- Vanilla Furniture Expanded - Power (by Veralin & Cozarkian) -->
  <Operation Class="PatchOperationSequence" MayRequire="OskarPotocki.VanillaFactionsExpanded.Core">
    <operations>
      <li Class="PatchOperationAdd" MayRequire="OskarPotocki.VanillaFactionsExpanded.Core">
        <xpath>Defs/ResearchProjectDef[defName="VFE_IndustrialGenerators" or defName="VFE_AdvancedBatteries" or defName="VFE_AdvancedPowerSources"
										or defName="VFE_NuclearPower" or defName="VFE_TidalPower"]/modExtensions/li[@Class="PawnsChooseResearch.ResearchCategory"]</xpath>
        <value>
          <constructionTech>1</constructionTech>
        </value>
      </li>
      <li Class="PatchOperationAdd" MayRequire="OskarPotocki.VanillaFactionsExpanded.Core">
        <xpath>Defs/ResearchProjectDef[defName="VFE_GasExtraction"]/modExtensions/li[@Class="PawnsChooseResearch.ResearchCategory"]</xpath>
        <value>
          <miningTech>1</miningTech>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>